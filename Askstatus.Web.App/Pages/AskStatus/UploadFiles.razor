@page "/admin/system/upload-files"
@attribute [Authorize(Permissions.System)]

<PageTitle>Askstatus - Upload files</PageTitle>
<MudFileUpload T="IBrowserFile" FilesChanged="UploadGoogleFile" Accept="gmail.com">
    <ActivatorContent>
        <MudButton Variant="Variant.Filled"
                   Color="Color.Primary"
                   StartIcon="@Icons.Material.Filled.CloudUpload">
            Upload Google Token Response File
        </MudButton>
    </ActivatorContent>
</MudFileUpload>
